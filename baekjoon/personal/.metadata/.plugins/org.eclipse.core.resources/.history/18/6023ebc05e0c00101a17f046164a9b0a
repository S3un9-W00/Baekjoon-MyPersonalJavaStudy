package no_1;
import java.util.*;

public class no_1 {s
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String[] word = (sc.nextLine().trim()).split(" ");
		
		int cnt = 0;
		
		for(int i = 0; i < word.length; i++) {
			cnt++;
		}
		
		System.out.println(cnt);
    }
}
