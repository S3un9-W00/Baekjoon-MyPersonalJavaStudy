package baekjoon;
import java.util.*;

public class baekjoon6 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s = sc.next();
		
		char[] c = s.toCharArray();
		
		
		
		for(char i = 'a'; i <= 'z'; i++) {
			for(int j = 0; j < c.length; j++) {
				if(c[j] == i) {
					System.out.print(j + " ");
				} else if(c[j] != i) {
					System.out.print("-1 ");
				}
			}
		}
				
	}
}
