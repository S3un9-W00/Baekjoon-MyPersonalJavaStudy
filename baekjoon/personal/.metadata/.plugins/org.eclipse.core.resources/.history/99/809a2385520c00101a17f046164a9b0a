package no_1;
import java.util.Scanner;
import java.util.Arrays;

public class no_1 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		
		int[] array = new int[n];
		
		int max = 0;
		
		int min = 0;
		
		for(int i = 0; i < array.length; i++) {
			int num = sc.nextInt();
			array[i] = num;
		}
		
		max = array[0];
		min = array[0];
		
		for(int i = 0; i < array.length; i++) {
			if(max <= array[i]) {
				max = array[i];
			}
			
			if(min >= array[i]) {
				min = array[i];
			}
		}
		
		
		System.out.println(min);
		
		System.out.println(max);
		
	}

}
